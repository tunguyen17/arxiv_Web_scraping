       " <name>Mohamad Kazem Shirani Faradonbeh</name>\n",
       " </author>\n",
       " <author>\n",
       " <name>Ambuj Tewari</name>\n",
       " </author>\n",
       " <author>\n",
       " <name>George Michailidis</name>\n",
       " </author>\n",
       " <comment>36 pages, 3 appendices</comment>\n",
       " <link href=\"http://arxiv.org/abs/1710.01852v1\" rel=\"alternate\" type=\"text/html\"/>\n",
       " <link href=\"http://arxiv.org/pdf/1710.01852v1\" rel=\"related\" title=\"pdf\" type=\"application/pdf\"/>\n",
       " <primary_category scheme=\"http://arxiv.org/schemas/atom\" term=\"cs.SY\"/>\n",
