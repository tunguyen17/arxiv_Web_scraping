       " <name>Mason A. Porter</name>\n",
       " </author>\n",
       " <link href=\"http://arxiv.org/abs/1709.08238v1\" rel=\"alternate\" type=\"text/html\"/>\n",
       " <link href=\"http://arxiv.org/pdf/1709.08238v1\" rel=\"related\" title=\"pdf\" type=\"application/pdf\"/>\n",
       " <primary_category scheme=\"http://arxiv.org/schemas/atom\" term=\"q-fin.TR\"/>\n",
